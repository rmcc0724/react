//Create a stateless functional component called childComponent
const ChildComponent = () => {
  return (
    <div>
      <p>I am the child</p>
    </div>
  );
};

//Create a parent component named ParentComponent
class ParentComponent extends React.Component {
  constructor(props) {
    super(props);
  }
  render() {
    return (
      <div>
        <h1>I am the parent</h1>
        { /* Include the child component nested in the parent */ }
<ChildComponent/>

      </div>
    );
  }
};
